@model LevelLearn.ViewModel.Institucional.Instituicao.CreateInstituicaoViewModel

<div class="modal-header">
    <h4 class="modal-title" id="myModal">Adicionar instituição</h4>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
</div>
<div class="modal-body">
    <form id="formCreate">
        <div class="row m-t-10">
            <div class="col-md-12">
                <label asp-for="Nome"></label><span class="required-field"></span>
                <input asp-for="Nome" class="form-control" />
            </div>
        </div>
        <div class="row m-t-10">
            <div class="col-md-12">
                <label asp-for="Descricao"></label>
                <textarea asp-for="Descricao" class="form-control"></textarea>
            </div>
        </div>
        <div class="row m-t-10">
            <div class="col-md-12">
                <label asp-for="Admins"></label>
                <select asp-for="Admins" asp-items="ViewBag.DropDownListProfessores" class="form-control my-select" multiple></select>
            </div>
        </div>
        <div class="row m-t-10">
            <div class="col-md-12">
                <label asp-for="Professores"></label>
                <select asp-for="Professores" asp-items="ViewBag.DropDownListProfessores" class="form-control my-select" multiple></select>
            </div>
        </div>
        <div class="row m-t-10">
            <div class="col-md-12">
                <label asp-for="Alunos"></label>
                <select asp-for="Alunos" asp-items="ViewBag.DropDownListAlunos" class="form-control my-select" multiple></select>
            </div>
        </div>
    </form>
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-danger" data-dismiss="modal">Fechar</button>
    <input type="button" id="btnCreate" value="Adicionar" onclick="createInstituicao()" class="btn btn-success" />
</div>
